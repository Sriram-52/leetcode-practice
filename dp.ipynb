   "source": [
    "function rob(nums: number[]): number {\n",
    "\tconst eventIndexSum = nums\n",
    "\t\t.filter((_, idx) => idx % 2 === 0)\n",
    "\t\t.reduce((a, b) => a + b, 0);\n",
    "\tconst oddIndexSum = nums\n",
    "\t\t.filter((_, idx) => idx % 2 === 1)\n",
    "\t\t.reduce((a, b) => a + b, 0);\n",
    "\treturn Math.max(eventIndexSum, oddIndexSum);\n",
    "}\n"
   ]
  }
